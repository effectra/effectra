{
    "name": "effectra/effectra",
    "type": "project",
    "description": "The Effectra Framework.",
    "keywords": ["framework", "aval"],
    "license": "MIT",
    "require": {
        "effectra/framework": "^2.2",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
